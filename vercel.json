{
    "version": 2,
    "builds": [
      {
        "src": "flaskr/app.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "15mb",
          "runtime": "Python3.9"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "flaskr/app.py"
      }
    ]
  }
  